generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider  = "postgresql"
}

model ContactMessage {
  id          String   @id @default(cuid())
  firstName   String
  lastName    String
  email       String
  company     String?
  projectType String?
  foundMe     String?
  message     String
  createdAt   DateTime @default(now())

  @@index([createdAt])
  @@index([email])
}
